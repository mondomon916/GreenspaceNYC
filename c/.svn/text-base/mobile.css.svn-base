#signin_menu {
    display:none;
    background-color:#66cccc;
    position:absolute;
    width:110px;
    height: 90px;
    z-index:100;
    text-align:left;
    padding:10px;
    padding-left: 15px;
    top: 103px; 
	right: 92px;
    color:#fff;
    font-size:11px;
}

#signin_menu input[type=text], #signin_menu input[type=password] {
    display:block;
    background-color: #66cccc;
    border: 1px solid #fff;
    font-size:10px;
    color: #fff;
    font-weight: bold;
    font-family: "news-gothic-std-1","news-gothic-std-2", Verdana, Geneva, sans-serif;
    margin:0 0 10px;
    width:100px;
}
#signin_menu p {
    margin:0;
}

#signin_menu a {
    color:#fff;
}

#signin_menu a.submit {
    color:#fff;
    text-transform: uppercase;
    font-size: 15px;
}
#signin_menu p.forgot, #signin_menu p.complete {
    clear:both;
    margin:8px 0;
    font-size:10px;
    font-family: "news-gothic-std-1","news-gothic-std-2", Verdana, Geneva, sans-serif;
    font-weight:bold;
}

#signin_menu p.forgot {
	margin-top: 3px;
}

#signin_menu p a {
    color:#fff;
}
#signin_submit {
    border:1px solid #fff;
    background-color: #ccc;
    color:#fff;
    font-size:10px;
    font-family: "news-gothic-std-1","news-gothic-std-2", Verdana, Geneva, sans-serif;
    font-weight:bold;
}








